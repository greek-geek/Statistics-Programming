library(Stats202A)
n <- 100
p <- 5
X    <- matrix(rnorm(n * p), nrow = n)
beta <- rnorm(p)
Y    <- 1 * (runif(n) < expit(X %*% beta))
logistic_beta <- LogisticRegression(X, Y)
print(logistic_beta)
print(glm(formula = Y ~ X + 0,  family=binomial("logit")))
n    <- 100
p    <- 3
## Simulate data from our assumed model.
## We can assume that the true intercept is 0
X    <- matrix(rnorm(n * p), nrow = n)
#beta <- matrix(1:p, nrow = p)
#Y    <- X %*% beta + rnorm(n)
## Save R's linear regression coefficients
#R_coef  <- coef(lm(Y ~ X))
R_coef  <- eigen(t(X) %*% X)
my_coef <- PCA(t(X) %*% X )
print(R_coef)
print(my_coef)
